namespace Deepstaging.Azure.Tables;

[AttributeUsage(AttributeTargets.Class, Inherited = false, AllowMultiple = false)]
internal class AzureTableEntityAttribute(string partitionKey, string rowKey, string tableName) : Attribute {
    public string PartitionKey { get; } = partitionKey;
    public string RowKey { get; } = rowKey;
    public string TableName { get; } = tableName;
}